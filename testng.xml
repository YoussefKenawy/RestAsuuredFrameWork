<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Regression">
    <listeners>
        <listener class-name="reporting.Setup"></listener>
    </listeners>
    <test name="DealAppTests" verbose="2" preserve-order="true">
        <classes>
            <class name="DealApp.ADS.CreateAd">
                <methods>
                    <include name="createAd"/>
                </methods>
            </class>
            <class name="DealApp.MarketRequests.CreateMarketingRequest"/>
            <class name="DealApp.MarketRequests.ChangeStatusByAdmin">
            <methods>
                <include name="changeStatusByAdmin_ToApprove"/>
            </methods>
        </class>
        <class name="DealApp.MarketRequests.CreateMarketingProposal"/>
            <class name="DealApp.MarketRequests.UpdateMarketingProposal"/>
            <class name="DealApp.MarketRequests.DeleteMarketingProposal"/>
            <class name="DealApp.MarketRequests.InteractWithMarketingProposals"/>
            <class name="DealApp.MarketRequests.GetAllProposalsForMarketingRequest"/>

        </classes>
    </test>
</suite>
